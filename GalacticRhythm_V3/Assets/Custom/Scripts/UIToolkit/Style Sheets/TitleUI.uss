.background {
    position: absolute;
    flex-grow: 1;
    align-self: center;
    width: 1920px;
    height: 100%;
    background-image: url("project://database/Assets/Custom/Art/Sprites/UI/Background.png?fileID=21300000&guid=3d8914830c62c014b9ab33e5ec720b63&type=3#Background");
    -unity-slice-left: 0;
    -unity-background-scale-mode: stretch-to-fill;
    justify-content: flex-start;
    align-items: stretch;
}

.container {
    position: absolute;
    flex-grow: 1;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-self: stretch;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0);
}

.txtTitle {
    background-color: rgba(255, 0, 0, 0);
    -unity-font: url("project://database/Assets/Custom/Art/Sprites/UI/Fonts/UpheavalPro.ttf?fileID=12800000&guid=c85fe93e6fb0ecd499db97aa4809d1bf&type=3#UpheavalPro");
    font-size: 175px;
    -unity-font-definition: initial;
    -unity-font-style: normal;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
    -unity-text-outline-width: 0.1px;
    letter-spacing: 10px;
    -unity-paragraph-spacing: 5px;
    top: 18%;
    width: 100%;
    flex-shrink: 0;
    align-items: stretch;
    align-self: center;
    min-height: 192px;
}

.btnStart {
    background-color: rgba(0, 255, 0, 0);
    -unity-font-definition: initial;
    -unity-font: url("project://database/Assets/Custom/Art/Sprites/UI/Fonts/Agdasima-Regular.ttf?fileID=12800000&guid=840b2655d67d9c449bf28a480a2c00af&type=3#Agdasima-Regular");
    font-size: 95px;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
    letter-spacing: 6px;
    opacity: 1;
    bottom: 5%;
    height: 25%;
    width: 100%;
    border-top-width: -1px;
    border-right-width: -1px;
    border-bottom-width: -1px;
    border-left-width: -1px;
}

.txtStudioName {
    background-color: rgba(0, 0, 0, 0);
    width: 100%;
    height: 40px;
    min-height: 2%;
    max-height: 5%;
    align-items: center;
    justify-content: flex-start;
    align-self: center;
    flex-shrink: 0;
    bottom: 2%;
    -unity-font-definition: initial;
    -unity-font: url("project://database/Assets/Custom/Art/Sprites/UI/Fonts/Agdasima-Regular.ttf?fileID=12800000&guid=840b2655d67d9c449bf28a480a2c00af&type=3#Agdasima-Regular");
    font-size: 48px;
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    letter-spacing: 2px;
    word-spacing: 5px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
}
